[setup]


ociuser= xxxxx
ocipasswd = xxxxx
ocidatabase = xxxxx.dbc.dk/xxxxx.dbc.dk

connect       = "host=xxxx.dbc.dk dbname=xxxx user=xxxx password=xxxx"

impcmd = "psql -h pgdrift.dbc.dk -U posthus -f $fil"

